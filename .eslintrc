{
  "extends": ["react-app", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/react-in-jsx-scope": 0,
    "prettier/prettier": [
      "error",
      {
        "doubleQuote": true,
				"printWidth": 175,
				"jsxBracketSameLine": true
      }
    ]
  }
}
